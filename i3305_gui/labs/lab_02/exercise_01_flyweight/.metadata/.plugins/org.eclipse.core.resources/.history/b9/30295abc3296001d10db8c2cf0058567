package exercise_01_flyweight;

import java.util.HashMap;
import DisplayCharacter;

public class Dictionary {
	
	private static final HasMap dictionary = new HashMap();
	
	public static Character getCharacter(char letter) {
		Character result = (Character) dictionary.get(letter);
		
		if (result == null) {
			result = new displayCharacter(letter);
		}
	}
}
